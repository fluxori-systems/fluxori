{
  "name": "fluxori-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --config src/testing/config/vitest.config.ts",
    "test:watch": "vitest --config src/testing/config/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config src/testing/config/vitest.config.ts",
    "test:ui": "vitest --ui --config src/testing/config/vitest.config.ts",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "dep:validate": "depcruise src",
    "dep:graph": "depcruise src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph.svg",
    "dep:components": "depcruise src --include-only \"^src/components\" --output-type dot | dot -T svg > component-dependencies.svg",
    "dep:archi": "depcruise src --output-type archi | dot -T svg > architecture.svg",
    "dep:check-circular": "depcruise --config --validate .dependency-cruiser.js src",
    "dep:check-modules": "depcruise --config --validate --focus \"^src/lib/(ui|motion)/\" .dependency-cruiser.js src",
    "dep:module-graph": "depcruise --config --output-type dot --focus \"^src/lib/(ui|motion|shared)/\" .dependency-cruiser.js src | dot -T svg > module-dependencies.svg",
    "validate-deps": "node scripts/validate-dependencies.js",
    "update-ui-deps": "node scripts/update-component-dependencies.js",
    "doc:generate": "typedoc --out docs/api-docs",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint-ts": "eslint --config .eslintrc.custom.json 'src/**/*.{ts,tsx}'",
    "lint-mantine": "eslint --config .eslintrc.mantine.json 'src/lib/ui/components/**/*.{ts,tsx}'",
    "fix-mantine-props": "node scripts/fix-mantine-props.js",
    "ts-error-report": "node ../scripts/typescript-error-report.js",
    "migrate-tests": "node scripts/migrate-tests.js"
  },
  "dependencies": {
    "@mantine/core": "^7.17.3",
    "@mantine/dropzone": "^7.17.3",
    "browser-image-compression": "^2.0.0",
    "@mantine/dates": "^7.17.3",
    "@mantine/form": "^7.17.3",
    "@mantine/hooks": "^7.17.3",
    "@mantine/modals": "^7.17.3",
    "@mantine/notifications": "^7.17.3",
    "@tabler/icons-react": "^3.1.0",
    "@tanstack/react-query": "^5.25.0",
    "@tanstack/react-query-devtools": "^5.25.0",
    "@tanstack/react-table": "^8.13.2",
    "axios": "^1.8.4",
    "chart.js": "^4.4.2",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.10",
    "gsap": "^3.12.7",
    "next": "^15.2.4",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.2.4",
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/nextjs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.1.3",
    "@vitest/ui": "^1.6.1",
    "cypress": "^13.7.1",
    "dependency-cruiser": "^16.10.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.2.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-mantine": "file:./eslint-plugin-mantine",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^10.4.5",
    "jsdom": "^23.0.1",
    "msw": "^2.2.7",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "storybook": "^8.0.4",
    "typedoc": "^0.28.2",
    "typedoc-plugin-merge-modules": "^7.0.0",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  }
}
